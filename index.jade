doctype html
html(lang="en")

  include ./includes/head.jade

  body#page-top.index
    #abp-nav.abp-hide
      // Navigation
      include ./includes/navbar.jade
      // Header
    header
      .logo
        img(src="img/logo.png", alt="A. Bloch Photography Logo")
      a(href="#about" class="btn btn-circle page-scroll")
        i.fa.fa-angle-double-down.animated
    // About Section
    section#about
      .container
        .row
          .col-lg-12.text-center
            h2.section-heading About
            h3.section-subheading.text-muted A Little About Me & My Photography.
        .row.ab-amanda
          .box
            .col-md-6
              img.img-responsive(src="img/amanda.jpg", alt="Amanda Bloch Prior")
            .col-md-6
              h3 What is A. Bloch Photography?
