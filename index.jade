      // Contact Section
      section#contact
        .container
          .row
            .col-lg-12.text-center
              h2.section-heading Connect & Contact
              h3.section-subheading.text-muted Lorem ipsum dolor sit amet consectetur.
              ul.list-inline.social-icon
                li
                  a.social(href="http://instagram.com/amandaebprior/", target="_blank")
                    img(src="img/lightbrown_instagram.png", alt="Instagram!")
                li
                  a.social(href="https://www.facebook.com/ABlochPhotography", target="_blank")
                    img(src="img/darkbrown_facebook.png", alt="Facebook!")
                li
                  a.social(href="http://www.pinterest.com/amandaebloch/", target="_blank")
                    img(src="img/lightbrown_pinterest.png", alt="Pinterest!")
          .row
            .col-lg-12
              form#contactForm(name="sentMessage", novalidate="")
                .row
                  .col-md-6
                    .form-group
                      input#name.form-control(type="text", placeholder="Your Name *", required="", data-validation-required-message="Please enter your name.")
                      p.help-block.text-danger
                    .form-group
                      input#email.form-control(type="email", placeholder="Your Email *", required="", data-validation-required-message="Please enter your email address.")
                      p.help-block.text-danger
                    .form-group
                      input#date.form-control(type="text", placeholder="Wedding Date (if applicable)")
                      p.help-block.text-danger
